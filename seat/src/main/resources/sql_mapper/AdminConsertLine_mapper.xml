<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="AdminCons
ertLine">
	<select id="CountLineid" parameterType="ConsertLine" resultType="String">
		select max(C_col_num) from ConsertHall_col where C_num=#{C_num} and C_hall_num=#{C_hall_num} 
	</select>
	<!-- 기본 메인정보 입력 쿼리 -->
	<insert id="AddLine" parameterType="ConsertLine">
		INSERT INTO ConsertHall_Col VALUES(#{C_col_num},#{C_num},#{C_hall_num},#{C_hall_name},#{C_col_name},#{C_row_count},#{C_col_priority})
	</insert>
	<select id="SelectLine" parameterType="ConsertLine" resultType="ConsertLine" >
	
	
	</select>
	

</mapper>